{
	"name": "console",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"start": "npx dotenv -e ./env/.env.production node bootstrap",
		"dev": "npx dotenv -e ./env/.env.development  vite dev",
		"build": "npx dotenv -e ./env/.env.production vite build",
		"staging": "NODE_ENV=staging npx dotenv -e ./env/.env.staging vite build --mode staging",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"dependencies": {
		"@repo/ui": "workspace:*",
		"tailwind-variants": "^0.2.1",
		"@internationalized/date": "^3.5.5",
		"bits-ui": "^0.21.13",
		"clsx": "^2.1.1",
		"cmdk-sv": "^0.0.18",
		"echarts": "^5.5.1",
		"embla-carousel-svelte": "^8.1.8",
		"express": "^4.19.2",
		"formsnap": "^1.0.1",
		"jsonwebtoken": "^9.0.2",
		"mode-watcher": "^0.4.1",
		"paneforge": "^0.0.5",
		"svelte-headless-table": "^0.18.2",
		"svelte-radix": "^1.1.0",
		"svelte-sonner": "^0.3.27",
		"sveltekit-superforms": "^2.16.1",
		"tailwind-merge": "^2.4.0",
		"vaul-svelte": "^0.3.2",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@repo/eslint-config": "workspace:*",
		"@sveltejs/adapter-auto": "^3.1.1",
		"@sveltejs/adapter-node": "^5.2.2",
		"@sveltejs/kit": "^2.5.2",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@typescript-eslint/eslint-plugin": "^7.1.0",
		"@typescript-eslint/parser": "^7.1.0",
		"autoprefixer": "^10.4.20",
		"eslint": "^8.57.0",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.2",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"svelte": "^4.2.12",
		"svelte-check": "^3.6.6",
		"tailwindcss": "^3.4.9",
		"tailwind-merge": "^2.4.0",
		"tslib": "^2.6.2",
		"typescript": "^5.6.2",
		"vite": "^5.1.4",
		"@iconify-json/lucide": "^1.1.205",
		"@iconify/svelte": "^4.0.2",
		"@types/eslint": "^9.6.0",
		"@types/jsonwebtoken": "^9.0.6",
		"dayjs": "^1.11.13",
		"dotenv-cli": "^7.4.2",
		"globals": "^15.0.0",
		"postcss": "^8.4.38"
	}
}
